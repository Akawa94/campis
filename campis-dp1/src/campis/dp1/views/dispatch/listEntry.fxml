<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="967.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="campis.dp1.controllers.dispatch.ListEntryController">
    <stylesheets>
        <URL value="@/campis/dp1/assets/css/listentries.css" />
    </stylesheets>
    <center>
      <AnchorPane prefHeight="800.0" prefWidth="1280.0">
         <children>
            <Label layoutX="44.0" layoutY="40.0" text="Lista de ingresos de mercancía">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <TableView fx:id="tablaEntries" layoutX="44.0" layoutY="121.0" prefHeight="360.0" prefWidth="750.0">
              <columns>
                  <TableColumn fx:id="idIngresCol" prefWidth="128.0" text="ID Ingreso" />
                  <TableColumn fx:id="prov_AlmCol" prefWidth="217.0" text="Proveedor/Almacén" />
                  <TableColumn fx:id="dateCol" prefWidth="182.0" text="Fecha de llegada" />
                  <TableColumn fx:id="reasonsCol" prefWidth="152.0" text="Motivo" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="createButton" layoutX="842.0" layoutY="266.0" mnemonicParsing="false" onAction="#goNewEntry" prefHeight="35.0" prefWidth="85.0" text="Nuevo" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
