<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="500.0" prefWidth="967.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="campis.dp1.controllers.roles.ListRoleController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <center>
      <AnchorPane prefHeight="500.0" prefWidth="967.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
            <Label layoutX="40.0" layoutY="35.0" prefHeight="43.0" prefWidth="524.0" text="Lista de Roles">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <TableView fx:id="tableRole" layoutX="40.0" layoutY="95.0" maxHeight="-Infinity" minHeight="0.0" prefHeight="360.0" prefWidth="750.0">
              <columns>
                <TableColumn fx:id="descriptionColumn" prefWidth="-1.0" text="Nombres" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button layoutX="840.0" layoutY="133.0" mnemonicParsing="false" onAction="#goCreateRole" prefHeight="35.0" prefWidth="85.0" text="Nuevo" />
            <Button layoutX="840.0" layoutY="205.0" mnemonicParsing="false" onAction="#goEditRole" prefHeight="35.0" prefWidth="85.0" text="Editar" />
            <Button layoutX="840.0" layoutY="275.0" mnemonicParsing="false" onAction="#deleteRole" prefHeight="35.0" prefWidth="85.0" text="Eliminar" />
            <Button layoutX="840.0" layoutY="343.0" mnemonicParsing="false" onAction="#goPermission" prefHeight="36.0" prefWidth="85.0" text="Permisos" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
